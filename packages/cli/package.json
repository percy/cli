{
  "name": "@percy/cli",
  "version": "1.0.0-beta.13",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "percy": "bin/run"
  },
  "files": [
    "bin",
    "index.js",
    "oclif.manifest.json"
  ],
  "scripts": {
    "lint": "eslint --ignore-path ../../.gitignore .",
    "postbuild": "oclif-dev manifest"
  },
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "percy",
    "plugins": [
      "@percy/cli-config",
      "@percy/cli-exec",
      "@percy/cli-build",
      "@percy/cli-snapshot",
      "@percy/cli-upload",
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "dependencies": {
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/plugin-plugins": "^1.9.0",
    "@percy/cli-build": "^1.0.0-beta.13",
    "@percy/cli-config": "^1.0.0-beta.13",
    "@percy/cli-exec": "^1.0.0-beta.13",
    "@percy/cli-snapshot": "^1.0.0-beta.13",
    "@percy/cli-upload": "^1.0.0-beta.13"
  }
}
