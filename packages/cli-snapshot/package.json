    "hooks": {
      "init": "./dist/hooks/init"
    }
  },
  "dependencies": {
    "@percy/cli-command": "^1.0.0-beta.50",
    "@percy/config": "^1.0.0-beta.50",
    "@percy/core": "^1.0.0-beta.50",
    "@percy/dom": "^1.0.0-beta.50",
    "@percy/logger": "^1.0.0-beta.50",
    "globby": "^11.0.1",
    "serve-handler": "^6.1.3",
    "yaml": "^1.10.0"
  }
}
