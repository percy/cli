{
  "name": "@percy/cli-snapshot",
  "version": "1.0.0-beta.36",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist",
    "oclif.manifest.json"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir dist --root-mode upward",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "postbuild": "oclif-dev manifest",
    "readme": "oclif-dev readme",
    "test": "cross-env NODE_ENV=test mocha",
    "test:coverage": "nyc yarn test"
  },
  "publishConfig": {
    "access": "public"
  },
  "mocha": {
    "require": "../../scripts/babel-register",
    "recursive": true,
    "timeout": 10000
  },
  "oclif": {
    "bin": "percy",
    "commands": "./dist/commands",
    "hooks": {
      "init": "./dist/hooks/init"
    }
  },
  "dependencies": {
    "@percy/cli-command": "^1.0.0-beta.36",
    "@percy/config": "^1.0.0-beta.36",
    "@percy/core": "^1.0.0-beta.36",
    "@percy/dom": "^1.0.0-beta.36",
    "@percy/logger": "^1.0.0-beta.36",
    "globby": "^11.0.1",
    "serve-handler": "^6.1.3",
    "yaml": "^1.10.0"
  }
}
