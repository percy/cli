{
  "name": "@percy/logger",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/percy/cli",
    "directory": "packages/logger"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist",
    "test/helpers.js",
    "test/client.js"
  ],
  "main": "./dist/index.js",
  "browser": "./dist/bundle.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./utils": "./dist/utils.js",
    "./test/helpers": "./test/helpers.js",
    "./test/client": "./test/client.js"
  },
  "imports": {
    "#logger": {
      "node": "./dist/logger.js",
      "default": "./dist/browser.js"
    }
  },
  "scripts": {
    "build": "node ../../scripts/build",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "test": "node ../../scripts/test",
    "test:coverage": "yarn test --coverage"
  },
  "rollup": {
    "output": {
      "name": "PercyLogger"
    }
  }
}
